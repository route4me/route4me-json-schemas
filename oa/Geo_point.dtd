{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "https://github.com/route4me/route4me-json-schemas/blob/master/oa/Geo_point.dtd",
	"title": "Geographic Point",
	"description": "Geographic point",
	"type": "array",
	"items": [
		{
			"type": "number",
			"minimum": -180,
			"maximum": 180,
			"title": "Longitude",
			"description": "Longitude"
		},
		{
			"type": "number",
			"minimum": -90,
			"maximum": 90,
			"title": "Latitude",
			"description": "Latitude"
		}
	]
}