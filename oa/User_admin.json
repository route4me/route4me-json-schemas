{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "https://raw.githubusercontent.com/route4me/route4me-json-schemas/master/oa/User_admin.json",
	"title": "User (admin)",
	"description": "JSON schema for a user (admin). Sub-schema",
	"type": "object",
	"properties": {
		"company_name": {
			"type": "string",
			"title": "Company Name",
			"description": "User's company name"
		},
		"phone_number": {
			"type": "string",
			"title": "Phone Number",
			"description": "Phone number of the user"
		},
		"locale": {
			"type": "string",
			"title": "Locale",
			"description": "Locale code ('en', 'fr', etc)"
		},
		"first_name": {
			"type": "string",
			"title": "First Name",
			"description": "First name"
		},
		"last_name": {
			"type": "string",
			"title": "Last Name",
			"description": "Last name"
		},
		"measurement": {
			"type": "integer",
			"title": "Measurement",
			"description": "Measurement"
		},
		"timezone": {
			"type": "string",
			"title": "Timezone",
			"description": "Timezone (e.g. 'UTC')"
		},
		"website": {
			"type": "string",
			"title": "Website",
			"description": "Website"
		},
		"email": {
			"type": "string",
			"title": "Email",
			"description": "Email"
		},
		"user_id": {
			"type": "string",
			"title": "User ID",
			"description": "User ID"
		}
	}
}