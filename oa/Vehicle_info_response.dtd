{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "https://github.com/route4me/route4me-json-schemas/blob/master/oa/Vehicle_info_response.dtd",
	"title": "Response Schema For Vehicle Info",
	"description": "Response Schema For the returned vehicle Info. Endpoint - https://api.route4me.com/api/v1/vehicles_info/all",
	"type": "object",
	"properties": {
		"last_position_received": {
			"type": "integer",
			"title": "Last Position Received",
			"description": "Last received vehilce position"
		},
		"vendor_id": {
			"type": "integer",
			"title": "Vendor ID",
			"description": "Unique ID of a telematics vendor"
		},
		"vehicle_model": {
			"type": "string",
			"title": "Vehicle Model",
			"description": "Vehicle model"
		},
		"external_id": {
			"type": "string",
			"title": "External ID",
			"description": "External ID"
		},
		"longitude": {
			"type": "number",
			"minimum": -180,
			"maximum": 180,
			"title": "Longitude",
			"description": "Longitude"
		},
		"vendor_hash_id": {
			"type": "string",
			"title": "Vendor HASH id",
			"description": "Hashed ID of a vendor"
		},
		"vendor_company_name": {
			"type": "string",
			"title": "Vendor Company Name",
			"description": "Vendor company name"
		},
		"vehicle_alias": {
			"type": "string",
			"title": "Vehicle Alias",
			"description": "Vehicle alias"
		},
		"vehicle_model_year": {
			"type": "integer",
			"title": "Vehicle Model Year",
			"description": "Vehicle model year"
		},
		"vehicle_make": {
			"type": "string",
			"title": "Vehicle Make",
			"description": "Vehicle maker"
		},
		"last_moved_at": {
			"type": "integer",
			"title": "Last Moved At",
			"description": "Last moved at"
		},
		"latitude": {
			"type": "number",
			"minimum": -90,
			"maximum": 90,
			"title": "Latitude",
			"description": "Latitude"
		},
		"member_id": {
			"type": "integer",
			"title": "Member ID",
			"description": "Member ID"
		},
		"timezone": {
			"type": "string",
			"title": "Timezone",
			"description": "Timezone"
		},
		"created_time": {
			"type": "integer",
			"title": "Created Time",
			"description": "Created time"
		},
		"vehicle_license_plate": {
			"type": "string",
			"title": "Vehicle License Plate",
			"description": "Vehicle license plate"
		},
		"vehicle_id": {
			"type": "string",
			"title": "Vehicle ID",
			"description": "Vehicle ID"
		}
	}
}