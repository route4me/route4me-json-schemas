{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "https://github.com/route4me/json-schemas/blob/master/Address.json",
	"title": "OA Address",
	"description": "Json sub-schema",
	"type": "object",
	"properties": {
		"alias": {
			"type": "string",
			"name": "alias",
			"title": "Address Alias",
			"description": "Address Alias"
		},
		"address": {
			"type": "string",
			"name": "address_name",
			"title": "Address Name",
			"description": "The route Address Line 1"
		},
		"lat": {
			"type": "number",
			"name": "lat",
			"title": "Latitude",
			"description": "Latitude",
			"minimum": -90,
			"maximum": 90
		},
		"route_destination_id": {
			"type": "integer",
			"name": "route_destination_id",
			"title": "Route Destination ID",
			"description": "Internal unique address identifier"
		},
		"time": {
			"type": "integer",
			"name": "time",
			"title": "Time",
			"description": "Service time (seconds)"
		},
		"sequence_no": {
			"type": "integer",
			"name": "sequence_no",
			"title": "Sequence No",
			"description": "The sequence number for the address"
		},
		"is_depot": {
			"type": "boolean",
			"name": "is_depot",
			"title": "Is Depot",
			"description": "This address is a depot",
			"default": "false"
		},
		"lng": {
			"type": "number",
			"name": "lng",
			"title": "Longitude",
			"description": "Longitude",
			"minimum": -180,
			"maximum": 180
		},
		"custom_fields": {
			"type": "array",
			"items": {"type": "string"},
			"description": "Any unrecognized fields from the original CSV upload will be found here",
			"name": "custom_fields",
			"title": "Custom Fields"
		}
	}
}
